<li>
  <a href="/<%= type %>/<%= id %>">
    <div class="info">
      <div class="edaItemSvg" id="edaItem_<%= type %>_<%= id %>" style="width: 150px; height: 150px;"></div>
      <div class="edaItemTitle"><%= title %></div>
    </div>
    <div class="moreInfo">
      <div class="readonlyField">
        <span class="title">Created By:</span><span class="value"><a href="/user/<%= createdBy.id %>"><%= createdBy.username %></a></span>
      </div>
      <div class="readonlyField">
        <span class="title">Modified Date:</span><span class="value"><%= sf('{0:MM/dd/yyyy}', new Date(modifiedDate)) %></span>
      </div>
    </div>
  </a>
</li>
