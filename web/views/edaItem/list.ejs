<table class="edaItemList table table-hover">
  <% items.forEach(function(item, i) { %>
  <tr>
    <td class="<%= i == 0 ? 'noTopBorder' : '' %>" onclick="navigateToEdaItem(<%= item.type %>, <%= item.id %>)">
      <div class="edaItemSvg listItemSvg" id="edaItem_<%= item.type %>_<%= item.id %>"></div>
      <textarea style="display: none;" id="edaItem_<%= item.type %>_<%= item.id %>_code"><%= item.code %></textarea>

      <div class="edaItemTitle"><%= item.title %></div>
      <% if(item.createdBy) { %>
      Created By: <a href="/user/<%= item.createdBy.id %>" class="edaItemCreatedBy"><%= item.createdBy.username %></a>
      <% } %>

      <div>Description:
        <div class="edaItemDescription"><%= item.description %></div>
      </div>
    </td>
  </tr>
  <% }); %>
</table>
